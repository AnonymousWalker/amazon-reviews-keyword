{
    "manifest_version": 3,
    "name": "Amazon Reviews Extension",
    "version": "1.0",
    "description": "customized extension",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "icons" : {
        "128" : "icon.png"
    }
}